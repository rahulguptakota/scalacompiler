section .data
message db "Register = %d", 10, 0
formatin: db "%d", 0

section .text
	global main
extern printf
extern scanf

main:
call func_1_main
mov  eax, 1
int  0x80

func_1_sub:
push ebp
mov ebp, esp
sub esp, 28
mov edx, [36 + ebp]
sub edx, [32 + ebp]
mov ecx, [28 + ebp]
push eax
push ebx
mov eax,ecx
mov ebx,[24 + ebp]
imul ebx
mov [24 + ebp],ebx
mov ecx,eax
pop eax
pop ebx
mov ebx, edx
add ebx, ecx
mov eax, ebx
add eax, [20 + ebp]
mov edx, [16 + ebp]
push eax
push ebx
mov eax,edx
mov ebx,[12 + ebp]
imul ebx
mov [12 + ebp],ebx
mov edx,eax
pop eax
pop ebx
mov ecx, eax
sub ecx, edx
mov edx, ecx
add edx, [8 + ebp]
push edx
mov esp, ebp
pop ebp
ret

func_1_main:
push ebp
mov ebp, esp
sub esp, 8
mov  eax, 1
push eax
mov  ebx, 2
push ebx
mov  ecx, 3
push ecx
mov  edx, 4
push edx
mov  edx, 5
push edx
mov  edx, 6
push edx
mov  edx, 7
push edx
mov  edx, 8
push edx
call func_1_sub
mov [-4 + ebp], eax
mov edx, [-4 + ebp]
mov esp, ebp
pop ebp
ret
