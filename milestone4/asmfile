section .data
message db "Register = %d", 10, 0
formatin: db "%d", 0

section .text
	global main
extern printf
extern scanf

main:
call func_1_main
mov  eax, 1
int  0x80

func_1_fibo:
push ebp
mov ebp, esp
sub esp, 60
mov edx, 0
lea eax,[-4 + ebp]
add eax,0
mov ebx,eax
add ebx,0
mov ebx, [ebx]
lea eax,[-4 + ebp]
add eax,4
mov ecx,eax
add ecx,1
mov ecx, [ecx]
lea eax,[-4 + ebp]
add eax,4
mov eax, [eax]
mov edx, eax
push edx
mov esp, ebp
pop ebp
ret

func_1_main:
push ebp
mov ebp, esp
sub esp, 8
mov  eax, 9
push eax
call func_1_fibo
mov [-4 + ebp], eax
mov edx, [-4 + ebp]
push eax
xor eax, eax
mov eax, edx
push eax
push message
call printf
add esp, 8
pop eax
mov esp, ebp
pop ebp
ret
