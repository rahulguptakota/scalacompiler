section	.text
   global _start   ;must be declared for linker (ld)
	
_start:	
xor ebx, ebx
add ebx, '0'
mov ecx, ebx
mov  edx,1      ;message length
mov  ebx, 1     ;file descriptor (stdout)
mov  eax, 4     ;system call number (sys_write)
int  0x80       ;call kernel
call func_1_main
mov  eax, 1     ;system call number (sys_exit)
int  0x80       ;call kernel

func_1_main:
push ebp
mov ebp, esp
sub esp, -32
mov edx, 10
mov ecx, 20
mov ebx, 0
mov eax, edx
add eax, ecx
mov [ebp - 8], edx
mov edx, eax
mov [ebp - 20], eax
mov [ebp - 16], ebx
mov [ebp - 12], ecx
mov [ebp - 24], edx



mov esp, ebp
pop ebp
ret

