section .data
message db "Register = %d", 10, 0
formatin: db "%d", 0
formatout: db "%d ", 0
formatdouble: db "%lf", 0

str18 db 'Double quote (\") is escaped',0
len_str18 equ $ - str18
f2 TIMES 0 DW  0

section .text
	global main
extern printf
extern scanf
extern fopen
extern fscanf
extern fprintf
extern fclose

main:
call func_1_main
mov  eax, 1
int  0x80

func_1_t:
push ebp
mov ebp, esp
sub esp, 0
push eax
push ebx
push ecx
push edx

xor eax, eax
push eax
push ebx
push ecx
push edx

xor eax, eax
mov eax, [12 + ebp]
push eax
push message
call printf
add esp, 8
pop edx
pop ecx
pop ebx
pop eax

mov esp, ebp
pop ebp
ret

func_1_main:
push ebp
mov ebp, esp
sub esp, 64
