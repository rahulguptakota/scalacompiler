section .data

section .text
	global _start

_start:
call func_1_main
mov  eax, 1
int  0x80

func_1_main:
push ebp
mov ebp, esp
sub esp, -32
mov edx, 10
mov ecx, 20
mov ebx, 0
mov eax, edx
add eax, ecx
mov [-8 + ebp], edx
mov edx, eax
push edx
push printFormat
mov [-20 + ebp], eax
mov [-16 + ebp], ebx
mov [-12 + ebp], ecx
mov [-24 + ebp], edx
call printf
add esp, 8
mov esp, ebp
pop ebp
ret
int_to_string:
add esi,9
mov byte [esi],0
mov ebx,10
.next_digit:
xor edx,edx
div ebx
add dl,'0'
dec esi
  mov [esi],dl
test eax,eax
jnz .next_digit
mov eax,esi
ret
