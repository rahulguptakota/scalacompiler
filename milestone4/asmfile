section .data
message db "Register = %d", 10, 0
formatin: db "%d", 0

section .text
	global main
extern printf
extern scanf

main:
call func_1_main
mov  eax, 1
int  0x80

func_1_fibo:
push ebp
mov ebp, esp
sub esp, 20
mov eax,0
lea eax,[-4 + ebp]
add eax,4
mov eax, [eax]
mov ebx,1
lea eax,[-4 + ebp]
add eax,4
mov eax, [eax]
mov [-20 + ebp],eax
mov eax, [-20 + ebp]
mov esp, ebp
pop ebp
ret
mov esp, ebp
pop ebp
ret

func_1_main:
push ebp
mov ebp, esp
sub esp, 8
mov  eax, 9
push eax
call func_1_fibo
mov [-4 + ebp], eax
mov eax,[-4 + ebp]
push eax
xor eax, eax
push eax
xor eax, eax
mov eax, [-8 + ebp]
push eax
push message
call printf
add esp, 8
pop eax
mov esp, ebp
pop ebp
ret
