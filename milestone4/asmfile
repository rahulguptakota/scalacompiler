section .data
message db "Register = %d", 10, 0
formatin: db "%d", 0
formatout: db "%d ", 0
formatdouble: db "%lf", 0
f2 TIMES 12 DW  0

section .text
	global main
extern printf
extern scanf
extern fopen
extern fscanf
extern fprintf
extern fclose

main:
call func_1_main
mov  eax, 1
int  0x80

func_1_t:
push ebp
mov ebp, esp
sub esp, 0
push eax
push ebx
push ecx
push edx

xor eax, eax
push eax
push ebx
push ecx
push edx

xor eax, eax
mov eax, [12 + ebp]
push eax
push message
call printf
add esp, 8
pop edx
pop ecx
pop ebx
pop eax

mov esp, ebp
pop ebp
ret

func_1_main:
push ebp
mov ebp, esp
sub esp, 60
mov eax, f2
add eax, 0
mov ebx, 1
mov [eax],ebx
mov eax, f2
add eax, 4
mov ebx, 2
mov [eax],ebx
mov eax, f2
add eax, 8
mov ebx, 3
mov [eax],ebx
lea eax, [-44 + ebp]
sub eax, 0
mov ebx, 1
mov [eax],ebx
lea eax, [-44 + ebp]
sub eax, 4
mov ebx, 2
mov [eax],ebx
lea eax, [-44 + ebp]
sub eax, 8
mov ebx, 3
mov [eax],ebx
mov eax,f2
add eax,4
mov eax, [eax]
mov edx, eax
mov [-56 + ebp], eax
push eax
push ebx
push ecx
push edx

xor eax, eax
mov eax, edx
push eax
push message
call printf
add esp, 8
pop edx
pop ecx
pop ebx
pop eax

mov [-60 + ebp], edx
mov esp, ebp
pop ebp
ret
