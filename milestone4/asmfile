section .data

str4 db 'Double quote (\") is escaped',0xa
len_str4 equ $ - str4

section .text
	global _start

_start:
call func_1_main
mov  eax, 1
int  0x80

func_1_main:
push ebp
mov ebp, esp
sub esp, -36
mov edx, 55
push eax
push ebx
push ecx
push edx
mov edx,len_str4
mov ecx,str4
mov ebx,1
mov eax,4
int 0x80
pop eax
pop ebx
pop ecx
pop edx
push eax
push ebx
push ecx
push edx
mov ecx,ebp
mov edx,8
sub ecx,edx
mov eax,3
mov ebx,2
mov edx,4
int 80h
pop eax
pop ebx
pop ecx
pop edx
mov [-8 + ebp], edx
mov esp, ebp
pop ebp
ret
