section .data

section .text
	global _start

_start:
call func_1_main
mov  eax, 1
int  0x80

func_1_main:
push ebp
mov ebp, esp
sub esp, -32
mov edx, 10
mov ecx, 20
mov ebx, 0
mov eax, edx
add eax, ecx
mov [-8 + ebp], edx
mov edx, eax
mov [-20 + ebp], eax
mov [-16 + ebp], ebx
mov [-12 + ebp], ecx
mov [-24 + ebp], edx
mov esp, ebp
pop ebp
ret
