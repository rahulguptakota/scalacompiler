.section .data

.section .text


str4:  .asciz Double quote (\") is escaped

printFormat:  .asciz "%d"

scanFormat:  .asciz "%d"

.global main

main:
movl $55, %edx
pushl $str4
movl %edx, -8(%ebp)
call printf
addl $4, %esp
pushl $10
pushl $printFormat
call printf
addl $8, %esp
pushl $12
pushl $printFormat
call printf
addl $8, %esp
pushl $a
pushl $scanFormat
call scanf
addl $8, %esp
movl %ebp, %esp
popl %ebp
ret
